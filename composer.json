{
    "name": "chiiya/tmdb-php",
    "description": "PHP SDK for the TMDB API",
    "keywords": [
        "chiiya",
        "tmdb-php"
    ],
    "homepage": "https://github.com/chiiya/tmdb-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Elisha Witte",
            "email": "github@chiiya.moe",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "antwerpes/data-transfer-object": "^1.0",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "blastcloud/guzzler": "^2.0",
        "chiiya/code-style-php": "^3.1",
        "phpro/grumphp": "^2.13",
        "phpunit/phpunit": "^12.0",
        "symfony/var-dumper": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Chiiya\\Tmdb\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chiiya\\Tmdb\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
